        / **********
        / NEW MODULE
        / **********
        .sidebar-module
          .sidebar-profile
            - unless current_admin.photo_url.to_s.empty?
              = image_tag current_admin.photo_url, class: 'avatar'
            - else
              %img.avatar{alt: "", src: "/assets/images/users/missing_user.png"}/
            %ul.sidebar-profile-list
              %li
                %h3 Hi, #{current_admin.name} 
              %li
                = link_to 'Profile', edit_admins_profile_path(current_admin)
                |
                = link_to destroy_user_session_path, :class => 'glyphicons power', :method => :delete do
                  %i
                  %span= t(:logout)
          / End .sidebar-profile
        / End .sidebar-module
        .sidebar-line
          / A seperator line
        / * Tabs can be removed, if so dont forget *
        / * to remove the .tab-pane divs(wrapper). *
        -#%ul.ext-tabs-sidebar
          %li.active
            %a{href: "#sidebar-tab-1"}
              %i.fa.fa-bars
              Navigation
          %li
            %a{href: "#sidebar-tab-2"}
              %i.fa.fa-folder
              Folders
        / End .ext-tabs-sidebar
        .tab-content
          #sidebar-tab-1.tab-pane.active.clearfix
            / **********
            / NEW MODULE
            / **********
            .sidebar-module
              %nav.sidebar-nav-v2
                =# params
                %ul
                  %li{class: "#{'page-arrow active-page' if params[:controller] == 'admins/dashboard'} "} 
                    = link_to admins_root_path do 
                      %i.fa.fa-dashboard
