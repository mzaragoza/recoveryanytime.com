                  %li{class: "#{'page-arrow active-page' if params[:controller] == 'admins/'} "} 
                    = link_to admins_root_path do 
                      %i.fa.fa-sun-o
                      Meetings
                  %li{class: "#{'page-arrow active-page' if params[:controller] == 'admins/'} "} 
