                  %li{class: "#{'page-arrow active-page' if params[:controller] == 'admins/admins'} "} 
                    = link_to admins_admins_path do 
                      %i.fa.fa-sun-o
                      Admins
                      %span.indicator-pill= Admin.is_active.count
                      %span.indicator-dot= Admin.all.count
                  %li{class: "#{'page-arrow active-page' if params[:controller] == 'admins/users'} "} 
                    = link_to admins_users_path do 
                      %i.fa.fa-sun-o
                      Users
                      %span.indicator-pill= User.is_active.count
                      %span.indicator-dot= User.all.count
                  %li{class: "#{'page-arrow active-page' if params[:controller] == 'admins/fellowships'} "} 
                    = link_to admins_fellowships_path do 
                      %i.fa.fa-sun-o
