!!!
%html{lang: "en"}
  %head
    %meta{charset: "utf-8"}/
    %title
      - unless yield(:title).to_s.empty?
        = yield (:title)
        |
      = t(:company_name).titleize
      |
      = t(:tagline).titleize
    %meta{content: "width=device-width, initial-scale=1, maximum-scale=1", name: "viewport"}/
    / <meta http-equiv="X-UA-Compatible" content="IE=9" />
    %link{href: "/assets/css/vendors/bootstrap.min.css", rel: "stylesheet"}/
    %link{href: "/assets/css/vendors/fontello.css", rel: "stylesheet"}/
    /[if IE 7]
      <link rel="stylesheet" href="/assets/css/fontello-ie7.css" >
    %link{href: "/assets/css/vendors/owl.carousel.css", rel: "stylesheet"}/
    %link{href: "/assets/css/vendors/slider.css", rel: "stylesheet"}/
    %link{href: "/assets/css/styles.css", rel: "stylesheet"}/
    %link{href: "/assets/css/media-queries.css", rel: "stylesheet"}/
    /[if lt IE 9]
      <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    /[if lt IE 9]
      <script src="http://css3-mediaqueries-js.googlecode.com/svn/trunk/css3-mediaqueries.js"></script>
  %body
    %header
      .header-top
        .container
          .row
            .col-md-5.contact
              = mail_to 'info@RecoveryAnytime.com' do
                %i.icon-mail>
                info@RecoveryAnytime.com
              =  link_to 'tel:9545922500' do 
                %i.icon-phone>
                954 592 2500
            .col-md-7.right
              %ul.social
                %li
                  = link_to 'https://vimeo.com/', target: '_blank' do
                    %i.icon-vimeo
                %li
                  = link_to 'https://Facebook.com/', target: '_blank' do
                    %i.icon-facebook
                %li
                  = link_to 'https://Pinterest.com/', target: '_blank' do
                    %i.icon-pinterest
                %li
                  = link_to 'https://Skype.com/', target: '_blank' do
                    %i.icon-skype
                %li
                  = link_to 'https://plus.google.com/', target: '_blank' do
                    %i.icon-gplus
                %li
                  = link_to 'https://Linkedin.com/', target: '_blank' do
                    %i.icon-linkedin
                %li
                  = link_to 'https://twitter.com/', target: '_blank' do
                    %i.icon-twitter
              -#%form.form-inline.form-search{role: "form"}
                .form-group
                  %label.sr-only{for: "search"} Search
                  %i.icon-search
                  %input#search.form-control{placeholder: "Search here...", type: "text"}/
                %button.btn.btn-default{type: "submit"} Sign in
              -#.options.dropdown
                %a#dLabel{"data-toggle" => "dropdown", href: "#"}
                  %i.icon-cog
                %ul.dropdown-menu{"aria-labelledby" => "dLabel", role: "menu"}
                  %li
                    %a{href: "#"}
                      %i.icon-cog
                      Settings
                  %li
                    %a{href: "#"}
                      %i.icon-user
                      Edit Profile
                  %li
                    %a{href: "#"}
                      %i.icon-help
                      Help
                  %li
                    %a{href: "#"}
                      %i.icon-reply
                      Logout
      %nav.navbar{role: "navigation"}
        .container
          / Brand and toggle get grouped for better mobile display
          .navbar-header
            %button.navbar-toggle{"data-target" => ".navbar-collapse", "data-toggle" => "collapse", type: "button"}
