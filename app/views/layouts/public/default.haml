              %span.icon-bar
              %span.icon-bar
              %span.icon-bar
            %a.navbar-brand{href: "#"}
              %img{alt: "//", src: "/assets/img/logo.png"}/
          / Collect the nav links, forms, and other content for toggling
          .collapse.navbar-collapse
            %ul.nav.navbar-nav
              %li.active
                %a{href: "index.html"} Home
              %li.dropdown
                %a.dropdown-toggle.active{"data-toggle" => "dropdown", href: "company.html"}
                  Company
                  %span.caret
